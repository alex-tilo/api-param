{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "expiresAt": {"type": "string"},
    "status": {"type": "string","enum": ["SUCCESS"]},
    "sessionToken": {"type": "string"},
    "_embedded": {
      "type": "object",
      "properties": {
        "user": {
          "type": "object",
          "properties": {
            "id": {"type": "string"},
            "passwordChanged": {"type": "string"},
            "profile": {
              "type": "object",
              "properties": {
                "login": {
                  "type": "string"
                },
                "firstName": {
                  "type": "string"
                },
                "lastName": {
                  "type": "string"
                },
                "locale": {
                  "type": "string"
                },
                "timeZone": {
                  "type": "string"
                }
              },
              "required": [
                "login",
                "firstName",
                "lastName",
                "locale",
                "timeZone"
              ]
            }
          },
          "required": [
            "id",
            "passwordChanged",
            "profile"
          ]
        }
      },
      "required": [
        "user"
      ]
    },
    "_links": {
      "type": "object",
      "properties": {
        "cancel": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            },
            "hints": {
              "type": "object",
              "properties": {
                "allow": {
                  "type": "array",
                  "items": [
                    {
                      "type": "string"
                    }
                  ]
                }
              },
              "required": ["allow"]
            }
          },
          "required": ["href","hints"]
        }
      },
      "required": ["cancel"]
    }
  },
  "required": [
    "expiresAt",
    "status",
    "sessionToken",
    "_embedded",
    "_links"
  ]
}
