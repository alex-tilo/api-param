{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ack":      {"type": "string", "enum": ["0"]},
    "data": {"type": "array","items": [
        {
          "type": "object",
          "properties": {
            "promoFactor": {"type": "string"},
            "promoMethod":  {"type": "string", "enum": ["NE", "CE", "NW", "PE"]},
            "promoPrice":     {"type": "number"},
            "promoMaxQty": {"type": "integer"},
            "promoMinQty":   {"type": "integer"},
            "cms_flag":   {"type": "boolean"}
          },
          "required": ["promoFactor","promoMethod","promoPrice","promoMaxQty","promoMinQty", "cms_flag"]
        }
      ]
    }
  },
  "required": ["ack","data"]
}