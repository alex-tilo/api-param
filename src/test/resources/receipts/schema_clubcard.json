

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "receipts": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "_id":          {"type": "string"},
            "posDateTime":  {"type": "string"},
            "dateString":   {"type": "string"},
            "banner":       {"type": "string"},
            "city":         {"type": "string"},
            "finalTotal":   {"type": "string"}
          },
          "required": ["_id","posDateTime","dateString","banner","city","finalTotal"]
        }
      ]
    }
  },
  "required": ["receipts"]
}
